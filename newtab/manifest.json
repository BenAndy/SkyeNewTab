{
  "version": "1.5.48",
  "name": "Skye New Tab",
  "short_name": "Skye New Tab",
  "description": "Replace your new tab page with a more modern page! Featuring time, dates, many background images, welcome messages and more!",
  "manifest_version": 2,
  "author": "Benjamin",
  "offline_enabled": true,
  "homepage_url": "http://benandy.github.io/skye",
  "incognito": "split",
  "background": {
    "scripts": ["background.js"]
  },
  "omnibox": {
    "keyword": "skye"
  },
  "icons": {
    "128": "content/favicon128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "cookies",
    "*://benandy.github.io/"
  ]
}
